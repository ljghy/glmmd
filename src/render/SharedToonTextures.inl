constexpr std::array<int, 256> base64DecodeTable()
{
    std::array<int, 256> table{};
    for (int i = 0; i < 64; ++i)
    {
        table["ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/"
                  [i]] = i;
    }
    return table;
}

template <size_t N = 3072>
constexpr std::array<uint8_t, N> decodeBase64(const char *base64)
{
    constexpr auto table = base64DecodeTable();

    std::array<uint8_t, N> out{};

    int valb = -8;
    for (size_t i = 0, val = 0, posOut = 0;
         i < std::char_traits<char>::length(base64) && table[base64[i]] != -1;
         i++)
    {
        val = (val << 6) + table[base64[i]];
        valb += 6;
        if (valb >= 0)
        {
            out[posOut++] = uint8_t((val >> valb) & 0xFF);
            valb -= 8;
        }
    }
    return out;
}

